<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>

	<typeAliases>
		<typeAlias alias="userRoleEntity" type="com.mugenunagi.amalbum.datastructure.entity.UserRoleEntity"/>
		<typeAlias alias="contentsRoleEntity" type="com.mugenunagi.amalbum.datastructure.entity.ContentsRoleEntity"/>
		<typeAlias alias="contentsGroupEntity" type="com.mugenunagi.amalbum.datastructure.entity.ContentsGroupEntity"/>
		<typeAlias alias="backupRuleEntity" type="com.mugenunagi.amalbum.datastructure.entity.BackupRuleEntity"/>
		<typeAlias alias="materialRoleEntity" type="com.mugenunagi.amalbum.datastructure.entity.MaterialRoleEntity"/>
		<typeAlias alias="materialEntity" type="com.mugenunagi.amalbum.datastructure.entity.MaterialEntity"/>
		<typeAlias alias="loginUserEntity" type="com.mugenunagi.amalbum.datastructure.entity.LoginUserEntity"/>
		<typeAlias alias="contentsEntity" type="com.mugenunagi.amalbum.datastructure.entity.ContentsEntity"/>
		<typeAlias alias="roleMasterEntity" type="com.mugenunagi.amalbum.datastructure.entity.RoleMasterEntity"/>
		<typeAlias alias="backupMasterEntity" type="com.mugenunagi.amalbum.datastructure.entity.BackupMasterEntity"/>

		<typeAlias alias="contentsGroupCondition" type="com.mugenunagi.amalbum.datastructure.condition.ContentsGroupCondition"/>
	</typeAliases>

	<mappers>
		<mapper resource="com/mugenunagi/amalbum/dao/ContentsGroupMapper.xml" />
		<mapper resource="com/mugenunagi/amalbum/dao/ContentsMapper.xml" />
		<mapper resource="com/mugenunagi/amalbum/dao/MaterialMapper.xml" />
		<mapper resource="com/mugenunagi/amalbum/dao/SequenceMapper.xml" />
		<mapper resource="com/mugenunagi/amalbum/dao/LoginUserMapper.xml" />
	</mappers>
</configuration>
